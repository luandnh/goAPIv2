SELECT vug.user_group, sum(IF(vl.length_in_sec>=0, vl.length_in_sec, 0)) as total_talk, COUNT(vl.phone_number) as total_call,
		(SELECT Count(vli.lead_id) FROM vicidial_list as vli LEFT JOIN vicidial_users vu ON  vu.user = vli.user WHERE vli.list_id = vl.list_id and  vli.app_status = 'NE' AND vu.user_group = vug.user_group  AND vli.modify_date BETWEEN '2021-06-01 00:00:00' AND '2021-06-01 23:59:00' ) as not_eligable,
		(SELECT Count(vli.lead_id) FROM vicidial_list as vli LEFT JOIN vicidial_users vu ON  vu.user = vli.user WHERE vli.list_id = vl.list_id and  vli.app_status = 'NI' AND vu.user_group = vug.user_group  AND vli.modify_date BETWEEN '2021-06-01 00:00:00' AND '2021-06-01 23:59:00' ) as not_interested,
		(SELECT Count(vli.lead_id) FROM vicidial_list as vli LEFT JOIN vicidial_users vu ON  vu.user = vli.user WHERE vli.list_id = vl.list_id and  vli.app_status = 'AC' AND vu.user_group = vug.user_group  AND vli.modify_date BETWEEN '2021-06-01 00:00:00' AND '2021-06-01 23:59:00' ) as app_created,
		(SELECT Count(vli.lead_id) FROM vicidial_list as vli LEFT JOIN vicidial_users vu ON  vu.user = vli.user WHERE vli.list_id = vl.list_id and  vli.app_status = 'AP' AND vu.user_group = vug.user_group  AND vli.modify_date BETWEEN '2021-06-01 00:00:00' AND '2021-06-01 23:59:00' ) as app_approved,
		(SELECT Count(vli.lead_id) FROM vicidial_list as vli LEFT JOIN vicidial_users vu ON  vu.user = vli.user WHERE vli.list_id = vl.list_id and  vli.STATUS != 'NEW' AND vu.user_group = vug.user_group  AND vli.modify_date BETWEEN '2021-06-01 00:00:00' AND '2021-06-01 23:59:00' ) as total_contacted
		FROM vicidial_user_groups vug left JOIN vicidial_log vl ON vug.user_group = vl.user_group
		WHERE  vl.call_date BETWEEN '2021-06-01 00:00:00' AND '2021-06-01 23:59:00' 
		GROUP BY vug.user_group